{
  "name": "livestock-bank",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run migrations",
    "start": "babel-node src/index.js",
    "dev": "nodemon --exec babel-node  src/index.js",
    "create-db": "npx sequelize-cli db:create",
    "drop-db": "npx sequelize-cli db:drop",
    "migrations": "npx sequelize-cli db:migrate",
    "undo": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo:all",
    "test": "cross-env NODE_ENV=test nyc mocha -r regenerator-runtime -r @babel/register --timeout 10000 --exit ./test/**/*.js",
    "pretest": "cross-env NODE_ENV=test npm run drop-db && cross-env NODE_ENV=test npm run create-db && cross-env NODE_ENV=test npm run migrations && cross-env NODE_ENV=test npm run seed",
    "coverage": "nyc --reporter=lcov npm test"
  },
  "keywords": [],
  "author": "Dusabeyezu Florentine (tytyne)",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.18.6",
    "@babel/node": "^7.18.6",
    "@babel/register": "^7.16.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.20.0",
    "caniuse-lite": "^1.0.30001365",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "i18n": "^0.15.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "method-override": "^3.0.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.7.7",
    "nodemailer-sendgrid": "^1.0.3",
    "nodemon": "^2.0.19",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "regenerator-runtime": "^0.13.9",
    "sequelize": "^6.21.3",
    "sequelize-cli": "^6.4.1",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
